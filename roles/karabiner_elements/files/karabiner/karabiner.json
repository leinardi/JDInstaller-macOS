        {
            "complex_modifications": {
                "rules": [
                    {
                        "description": "Linux: F5 to reload in browsers",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^com\\.microsoft\\.edgemac$",
                                            "^com\\.google\\.Chrome$",
                                            "^com\\.brave\\.Browser$",
                                            "^com\\.apple\\.Safari$"
                                        ],
                                        "type": "frontmost_application_if"
                                    }
                                ],
                                "from": { "key_code": "f5" },
                                "to": [
                                    {
                                        "key_code": "r",
                                        "modifiers": ["left_command"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Linux: F2 renames files and Enter opens files in Finder",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^com\\.apple\\.finder$"
                                        ],
                                        "type": "frontmost_application_if"
                                    }
                                ],
                                "from": {
                                    "key_code": "f2",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [
                                    {
                                        "set_variable": {
                                            "name": "rename_from_f2",
                                            "value": 1
                                        }
                                    },
                                    { "key_code": "return_or_enter" }
                                ],
                                "to_after_key_up": [
                                    {
                                        "set_variable": {
                                            "name": "rename_from_f2",
                                            "value": 0
                                        }
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^com\\.apple\\.finder$"
                                        ],
                                        "type": "frontmost_application_if"
                                    },
                                    {
                                        "name": "rename_from_f2",
                                        "type": "variable_unless",
                                        "value": 1
                                    }
                                ],
                                "from": {
                                    "key_code": "return_or_enter",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [
                                    {
                                        "key_code": "down_arrow",
                                        "modifiers": ["command"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
